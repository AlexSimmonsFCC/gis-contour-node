    "parameters": {
		"tvFilenumber": {
            "name":"tvFilenumber",
            "in":"path",
            "description":"filenumber of the station (example: BLTT-1002)",
            "required":true,
            "type":"string",
			"default": "BLTT-1002"
        },
		"tvApplicationId": {
            "name":"tvApplicationId",
            "in":"path",
            "description":"application ID of the station (example: 8)",
            "required":true,
            "type":"integer",
			"default": "8"
        },
		"fmFilenumber": {
            "name":"fmFilenumber",
            "in":"path",
            "description":"File number of the station (example: BLED-1533)",
            "required":true,
            "type":"string",
			"default": "BLED-1533"
        },
		"fmApplicationId": {
            "name":"fmApplicationId",
            "in":"path",
            "description":"application ID of the station (example: 750)",
            "required":true,
            "type":"integer",
			"default": "750"
        },
		"antennaId": {
            "name": "antennaId",
            "in":"path",
            "description":"antenna ID of the station (example: 100003)",
            "required":true,
            "type":"integer",
			"default": "100003"
        },
		"stationClass": {
            "name": "stationClass",
            "in":"path",
            "description":"class of the station (can be A, B, C, or D)",
            "required":true,
            "type":"string",
			"default": "B"
        },
		"timePeriod": {
            "name": "timePeriod",
            "in": "path",
            "description": "applicable time period (can only be daytime for now)",
            "required": true,
            "type": "string",
			"default": "daytime"
        }   
